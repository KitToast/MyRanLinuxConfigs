startup_message off
escape s-space

alias nmcli exec nmcli_dmenu

bind t exec urxvt
bind c kill
bind f exec google-chrome-stable
bind r exec
bind semicolon colon
bind h hsplit
bind v vsplit
bind e shrink
bind F exec xfe
bind b exec ~/ratpoison/bat.sh

bind s-1 exec rpws 1
bind s-2 exec rpws 2
bind s-3 exec rpws 3

set bargravity se

exec rpws init 3 -k 
exec feh --bg-fill Pictures/the_melancholy_of_haruhi_suzum_2560x1600_animemay.com.jpg
exec compton & 
#exec xbacklight -dec 40
